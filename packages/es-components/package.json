{
  "name": "es-components",
  "version": "19.0.57",
  "description": "React components built for Exchange Solutions products",
  "repository": "https://github.com/wtw-im/es-components",
  "module": "lib/index.js",
  "main": "cjs/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:es5 && npm run build:es6 && npm run webpack",
    "build:es5": "rollup -c rollup.es5.js",
    "build:es6": "rollup -c rollup.es6.js",
    "ci": "npm run lint && npm test",
    "lint": "eslint src",
    "prepublishOnly": "npm run build",
    "prettify": "prettier --single-quote --write \"src/**/*.js\"",
    "styleguide-build": "styleguidist build",
    "start": "styleguidist server",
    "test": "jest",
    "webpack": "webpack --config webpack-components-bundle.config.js"
  },
  "author": "Willis Towers Watson - Individual Marketplace",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "./config/jest-shims.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/import-matchers.js"
    ],
    "testPathIgnorePatterns": [
      "es6",
      "lib"
    ],
    "testRegex": "specs\\.js$",
    "testURL": "http://localhost/"
  },
  "prettier": {
    "singleQuote": true
  },
  "peerDependencies": {
    "react": ">=16.8",
    "styled-components": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.2",
    "babel-preset-wtw-im": "^2.3.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^2.1.1",
    "es-components-via-theme": "file:../es-components-via-theme",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-exchange-solutions": "^7.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^0.9.0",
    "jest": "^24.1.0",
    "jest-cli": "^24.8.0",
    "jest-dom": "^2.1.0",
    "mockdate": "^2.0.1",
    "prettier": "^1.14.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-styleguidist": "^8.0.6",
    "react-test-renderer": "^16.3.0",
    "react-testing-library": "^6.0.0",
    "regenerator-runtime": "^0.12.1",
    "request": "^2.88.0",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
    "style-loader": "^0.13.1",
    "styled-components": "^4.2.1",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.3.4",
    "date-fns": "^2.0.0-alpha.31",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react-animate-height": "^2.0.9",
    "react-datepicker": "^2.5.0",
    "react-overlays": "^1.2.0",
    "react-popper": "^1.3.3",
    "react-text-mask": "^5.4.3",
    "react-transition-group": "^4.0.0",
    "text-mask-addons": "3.7.2",
    "tinycolor2": "^1.4.1",
    "uncontrollable": "^6.0.0"
  }
}
